{
  "name": "n8n-nodes-planaday",
  "version": "1.0.0",
  "description": "n8n custom node for Planaday API integration",
  "types": "dist/index.d.ts",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "docker-build": "cd .docker && mkdir data && sudo chown -R 1000:1000 data && docker compose up",
    "start": "pnpm build && docker restart n8n-dev && docker logs n8n-dev -f"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/PlanadayApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Planaday/Planaday.node.js"
    ]
  },
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "planaday",
    "api"
  ],
  "license": "MIT",
  "homepage": "https://n8n.io",
  "author": {
    "name": "n8n Community",
    "email": "info@n8n.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/planaday-nl/n8n-nodes-planaday.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
